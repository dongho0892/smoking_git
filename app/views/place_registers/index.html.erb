<p id="notice"><%= notice %></p>

<h1>Place Registers</h1>

<table>
  <thead>
    <tr>
      <th>Type</th>
      <th>Area name</th>
      <th>Description</th>
      <th>Address</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @place_registers.each do |place_register| %>
      <tr>
        <td><%= place_register.type %></td>
        <td><%= place_register.area_name %></td>
        <td><%= place_register.description %></td>
        <td><%= place_register.address %></td>
        <td><%= place_register.latitude %></td>
        <td><%= place_register.longitude %></td>
        <td><%= link_to 'Show', place_register %></td>
        <td><%= link_to 'Edit', edit_place_register_path(place_register) %></td>
        <td><%= link_to 'Destroy', place_register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Place Register', new_place_register_path %>
